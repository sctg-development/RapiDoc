  "repository": {
    "type": "git",
    "url": "https://github.com/sctg-development/RapiDoc"
  },
  "license": "MIT",
  "keywords": [
    "OpenAPI",
    "OpenAPI Specification",
    "Swagger",
    "JSON-Schema",
